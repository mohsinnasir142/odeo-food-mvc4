<!--  ATmodel is keyword .Not a Instance of model --->
@model OdeoFoodMVC4.Models.AboutModel
@{
    ViewBag.Title = "About";
}

<hgroup class="title">
    <h1>@Model.Name</h1>
    <h2>@Model.Location</h2>
</hgroup>
<!--AtModel is represent the model of AboutModel--->


<aside>
    <h3>Aside Title</h3>
    <p>
        Use this area to provide additional information.
    </p>
    <ul>
        <li>@Html.ActionLink("Home", "Index", "Home")</li>
        <li>@Html.ActionLink("About", "About", "Home")</li>
        <li>@Html.ActionLink("Contact", "Contact", "Home")</li>
    </ul>
</aside>